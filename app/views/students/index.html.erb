<h1>Student Search Results</h1>
<%= form_tag :students, :method => 'get' do %>
  <%= label_tag :student_name, "Student Name: " %>
  <%= text_field_tag :student_name %>
  <%= submit_tag "Search" %>
<% end %>

<% @students.each do |student| %>
  <%= render partial: 'student', locals: {student: student} %>
<% end %>
