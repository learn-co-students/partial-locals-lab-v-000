<%= form_tag students_path, method: :get do %>
    <%= text_field_tag :search_query, params["search_query"], placeholder: "Search Students" %>
    <%= submit_tag "Search", :name => nil %>
<% end %>


<% @students.each do |student| %>
<%= render partial: "student", locals: {student: student} %>
<%#are locals really necessary here? Same for "classroom" in classrooms#show %>
<% end %>