<h2>Student Search</h2>
<%= form_tag(students_path, :method => "get", id: "search-form") do %>  <!-- found online Create a Simple Search Form in Rails-->

<%= text_field_tag :search, params[:search], placeholder: "Search Students" %><br>

<%= submit_tag "Search", :name => nil  %>
<% end %>


<% @students.each do |student| %>
<%= render partial: "student", locals: {student: student} %><br>
<% end %>
