<h1>Student Search Results</h1>
<%= form_tag("/index", method: "get") do %>
    <%= label_tag :search, "Search students" %>
    <%= text_field_tag :search %>
    <%= submit_tag %>
<% end %>

<%= @students.each do |student| %>
    <%= render partial: 'student', locals: {student: student} %>
<% end %>