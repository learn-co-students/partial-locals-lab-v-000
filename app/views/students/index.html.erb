<h1>All Students</h1>
<span style="float: right">
  <%= form_tag students_path do %>
    <%# <input type="text" name="query[name]" placeholder="search for student"> %>
    <%= text_field_tag :params_query %>
    <%= submit_tag "search" %>
  <% end%>
</span>

<% @students.each do |student| %>
  <%= render partial: 'student', locals: {student: student} %>
<% end %>