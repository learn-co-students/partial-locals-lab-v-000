<h1>Current Students</h1>

<h3>Student Search:</h3>
<%= form_tag(students_path, method: :get) do %>
    <p>
      <%= text_field_tag :search, params[:search] %>
      <%= submit_tag "Search", name: nil %>
    </p>
<% end %>

<% @students.each do |s| %>
    <%= render partial: 'student', locals: {student: s} %>
<% end %>