<%= form_tag students_path do %>
  <label>Query:</label><br>
  <%= text_field_tag :query %><br>
  <%= submit_tag "Submit Query" %>
<% end %>

<% @students.each do |student| %>
<%= student.name %>
<% end %>


<% Student.all.each do |student| %>
<%= render partial: 'student', locals: {student: student} %>
<%end%>
