<h1>Student Search</h1>

<%= form_tag students_path, method: "get" do %>
  <label for="">Search Students:</label>
  <%= text_field_tag :search, params[:search_query] %>
  <%= submit_tag "Search"%>
<% end %>

<% @students.each do |student|%>
  <%= render partial: "student", locals: { student: student } %>
<%end%>
