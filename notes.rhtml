<% classroom_student.each do |student| %>

<ul>
  <li>
    Name: <%= student.name %>
  </li>
  <li>
    Birthday: <%= student.birthday.strftime("%m/%d/%Y")  %>
  </li>
  <li>
    Hometown: <%= student.hometown.capitalize %>
  </li>
</ul>


<%= render partial: 'students/student', locals: {classroom_student: @classroom.students} %>


<%= render partial: 'student', locals: {students: Student.search()} %>
